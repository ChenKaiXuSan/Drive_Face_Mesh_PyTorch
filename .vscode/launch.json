{
  "configurations": [
    {
      "name": "Python 调试程序: 模块",
      "type": "debugpy",
      "request": "launch",
      "module": "enter-your-module-name"
    },
    {
      "name": "prepare_mesh_2d",
      "type": "debugpy",
      "request": "launch",
      "module": "prepare_mesh.main",
      "justMyCode": true,
    },
    {
      "name": "calibrate_cameras",
      "type": "debugpy",
      "request": "launch",
      "module": "camera_calibration.main",
    },
    {
      "name": "prepare_mesh_3d",
      "type": "debugpy",
      "request": "launch",
      "module": "mesh_triangulation.main",
      "args": [
        "paths.mesh_path=/workspace/data/npz_debug",
        "debug=True"
      ]
    },
    {
      "name": "prepare_sam3d_body",
      "type": "debugpy",
      "request": "launch",
      "module": "SAM3Dbody.main",
      "cwd": "/workspace/code",
      "python": "/opt/conda/envs/sam_3d_body/bin/python",
      "console": "integratedTerminal",
      "args": [
        "infer.gpu=[0,1]",
        "infer.person_list=[19]",
        "infer.env_list=[all]",
        "infer.views_list=[front]",
      ]
    },
    {
      "name": "head3d_fuse",
      "type": "debugpy",
      "request": "launch",
      "module": "head3D_fuse.main",
      "cwd": "/workspace/code",
      "python": "/opt/conda/envs/sam_3d_body/bin/python",
      "console": "integratedTerminal",
      "args": [
        "infer.workers=1",
        "infer.person_list=[01]",
        "infer.env_list=[all]",
      ]
    }
  ]
}